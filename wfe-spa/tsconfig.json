{
    "compilerOptions": {
      "outDir": "./dist/",
      "types": [
        "vuetify",
      ],
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "src/*"
        ]
      },
      "moduleResolution": "node",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "strict": true,
      "allowJs": true
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.vue",
    ],
    "exclude": [
      "node_modules",
    ]
}