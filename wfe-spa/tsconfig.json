{
    "compilerOptions": {
      "outDir": "./dist/",
      "types": [
        "vuetify",
        "webpack-env",
        "lodash",
        "webfontloader",
      ],
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "src/*"
        ]
      },
      "moduleResolution": "node",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "ESNext",
      "target": "ESNext",
      "strict": true,
      "allowJs": true
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.vue",
    ],
    "exclude": [
      "node_modules",
    ]
}